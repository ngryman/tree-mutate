{
  "name": "tree-mutate",
  "version": "0.3.0",
  "description": "n-ary tree mutation library.",
  "author": "Nicolas Gryman <ngryman@gmail.com> (http://ngryman.sh/)",
  "license": "MIT",
  "repository": "ngryman/tree-mutate",
  "main": "dist/tree-mutate.node.js",
  "browser": "dist/tree-mutate.browser.js",
  "module": "index.js",
  "jsnext:main": "index.js",
  "engines": {
    "node": ">=4"
  },
  "files": [
    "index.js",
    "dist/"
  ],
  "scripts": {
    "lint": "meta lint",
    "pretest": "npm run lint -s",
    "test": "meta test",
    "dev": "meta dev",
    "coverage": "meta coverage",
    "see-coverage": "meta see-coverage",
    "check-coverage": "meta check-coverage",
    "docs": "meta docs",
    "prebuild": "npm run lint -s",
    "build": "meta build",
    "prepublish": "npm run build -s"
  },
  "precommit": [
    "test",
    "check-coverage",
    "docs"
  ],
  "keywords": [
    "mutate",
    "morph",
    "monomorphism",
    "isomorphism",
    "tree",
    "n-ary tree",
    "k-ary tree",
    "n-way tree",
    "multiway tree",
    "rose tree",
    "generic",
    "agnostic",
    "mutable",
    "recursive",
    "map",
    "filter",
    "pluck",
    "transform",
    "functor"
  ],
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "env": {
      "development": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ],
        "sourceMaps": "inline"
      }
    }
  },
  "eslintConfig": {
    "extends": "ngryman"
  },
  "dependencies": {
    "tree-crawl": "^0.5.0"
  },
  "devDependencies": {
    "clone": "^2.0.0",
    "meta-dev": "^0.4.0",
    "pre-commit": "^1.1.3",
    "spy": "^0.1.3"
  }
}
